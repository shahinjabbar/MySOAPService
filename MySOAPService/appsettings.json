{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "/Log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": null,
          "shared": true
        }
      }
    ]
  }
}
